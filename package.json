  },
  "main": "index",
  "dependencies": {
    "archiver": "^3.0.0",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "express": "^4.17.0",
    "fs-extra": "^8.0.1",
    "gtfs": "1.8.6",
    "js-beautify": "^1.10.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.5.10",
    "morgan": "^1.9.1",
    "progress": "^2.0.3",
    "pug": "^2.0.3",
    "sanitize-filename": "^1.6.1",
    "simplify-geojson": "^1.0.3",
    "timer-machine": "^1.1.0",
    "untildify": "^4.0.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">= 8.11.1"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  },
  "false": {}
