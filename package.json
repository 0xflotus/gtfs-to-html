{
  "name": "gtfs-to-html",
  "description": "Build transit timetables in HTML from a GTFS file",
  "version": "0.13.9",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "timetables"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [
    "Evan Siroky",
    "Nathan Selikoff",
    "Aaron Antrim <aaron@trilliumtransit.com>",
    "thomastrillium",
    "hkvlhm"
  ],
  "repository": "git://github.com/blinktaginc/gtfs-to-html",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-html/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "bin": {
    "gtfs-to-html": "bin/gtfs-to-html.js"
  },
  "main": "index",
  "dependencies": {
    "@mapbox/geojson-merge": "^1.0.2",
    "archiver": "^2.1.1",
    "cli-table": "^0.3.1",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "gtfs": "1.5.3",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "mongoose": "5.0.9",
    "morgan": "^1.9.0",
    "pug": "^2.0.0",
    "sanitize-filename": "^1.6.1",
    "simplify-geojson": "^1.0.3",
    "timer-machine": "^1.1.0",
    "untildify": "^3.0.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "xo": "^0.20.3"
  },
  "engines": {
    "node": ">= 8.1.4"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  },
  "false": {}
}
